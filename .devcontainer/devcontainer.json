{
    "name": "MongoDB Developer Instance",
    "dockerComposeFile": "docker-compose.yml",
    "service": "mongodb",
    "workspaceFolder": "/workspaces",
    "forwardPorts": [
        27017
    ],
    "hostRequirements": {
        "cpus": 1
    },
    "customizations": {
        "codespaces": {
            "openFiles": []
        },
        "vscode": {
            "extensions": [
                "mongodb.mongodb-vscode"
            ]
        }
    }
}